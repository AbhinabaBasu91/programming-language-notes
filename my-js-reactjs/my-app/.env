REACT_APP_SECRET_CODE=abcdef

# Expand variables already on your machine, get the environment variable npm_package_version
REACT_APP_VERSION=$npm_package_version
# also works:
# REACT_APP_VERSION=${npm_package_version}

# expand variables local to the current
DOMAIN=www.example.com
REACT_APP_FOO=$DOMAIN/foo
REACT_APP_BAR=$DOMAIN/bar

